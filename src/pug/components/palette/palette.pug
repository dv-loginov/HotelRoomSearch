include ../color-info/color-info
-
    var colorInfo = [
        {classMod:"dark-shade-100", name: "Dark Shade 100%", code:"#1F2041"},
        {classMod:"dark-shade-75", name: "Dark Shade 75%", code:"#1F2041"},
        {classMod:"dark-shade-50", name: "Dark Shade 50%", code:"#1F2041"},
        {classMod:"dark-shade-25", name: "Dark Shade 25%", code:"#1F2041"},
        {classMod:"dark-shade-5", name: "Dark Shade 5%", code:"#1F2041"},
        {classMod:"purple", name: "Purple", code:"#BC9CFF"},
        {classMod:"green", name: "Green", code:"#6FCF97"},
    ]

.palette
    each val in colorInfo
        +color-info(val.classMod, val.name, val.code)
